<!-- plugins/tiDoctrineOAuthPlugin/package.xml -->
<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.4.1" version="2.0"
   xmlns="http://pear.php.net/dtd/package-2.0"
   xmlns:tasks="http://pear.php.net/dtd/tasks-1.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
   http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0
   http://pear.php.net/dtd/package-2.0.xsd"
>
  <name>tiDoctrineOAuthPlugin</name>
  <channel>plugins.symfony-project.org</channel>
  <summary>A OAuth Server plugin.</summary>
  <description>A OAuth Server plugin.</description>
  <lead>
    <name>Emeric KASBARIAN</name>
    <user>emerick42</user>
    <email>emeric@tillid.fr</email>
    <active>yes</active>
  </lead>
  <date>2012-04-13</date>
  <version>
    <release>0.0.1</release>
    <api>0.0.1</api>
  </version>
  <stability>
    <release>beta</release>
    <api>beta</api>
  </stability>
  <license uri="http://www.symfony-project.com/license">
    MIT license
  </license>
  <notes />
 
  <contents>
    <dir name="/">
      <file role="data" name="README" />
      <file role="data" name="LICENSE" />
      <file role="data" name="VERSION" />
      <dir name="config">
        <dir name="doctrine">
          <file role="data" name="schema.yml" />
        </dir>
        <file role="data" name="routing.yml" />
        <file role="data" name="view.yml" />
      </dir>
      <dir name="data">
        <dir name="fixtures">
          <file role="data" name="fixtures.yml.sample" />
        </dir>
      </dir>
      <dir name="i18n">
        <file role="data" name="ti_oauth_server.fr.xml" />
      </dir>
      <dir name="lib">
        <dir name="base">
          <dir name="vendor">
            <dir name="oauth">
              <file role="data" name="LICENSE.txt" />
              <file role="data" name="OAuth.php" />
            </dir>
          </dir>
          <file role="data" name="tiOAuthDataStore.class.php" />
          <file role="data" name="tiOAuthServer.class.php" />
        </dir>
        <dir name="filter">
          <dir name="doctrine">
            <file role="data" name="PluginOAuthServerAccessTokensFormFilter.class.php" />
            <file role="data" name="PluginOAuthServerConsumersFormFilter.class.php" />
            <file role="data" name="PluginOAuthServerNoncesFormFilter.class.php" />
            <file role="data" name="PluginOAuthServerRequestTokensFormFilter.class.php" />
          </dir>
        </dir>
        <dir name="form">
          <dir name="doctrine">
            <file role="data" name="PluginOAuthServerAccessTokensForm.class.php" />
            <file role="data" name="PluginOAuthServerConsumersForm.class.php" />
            <file role="data" name="PluginOAuthServerNoncesForm.class.php" />
            <file role="data" name="PluginOAuthServerRequestTokensForm.class.php" />
          </dir>
        </dir>
        <dir name="model">
          <dir name="doctrine">
            <file role="data" name="PluginOAuthServerAccessTokens.class.php" />
            <file role="data" name="PluginOAuthServerAccessTokensTable.class.php" />
            <file role="data" name="PluginOAuthServerConsumers.class.php" />
            <file role="data" name="PluginOAuthServerConsumersTable.class.php" />
            <file role="data" name="PluginOAuthServerNonces.class.php" />
            <file role="data" name="PluginOAuthServerNoncesTable.class.php" />
            <file role="data" name="PluginOAuthServerRequestTokens.class.php" />
            <file role="data" name="PluginOAuthServerRequestTokensTable.class.php" />
          </dir>
        </dir>
			</dir>
			<dir name="modules">
				<dir name="tiOAuthServer">
					<dir name="actions">
						<file role="data" name="actions.class.php" />
					</dir>
					<dir name="config">
						<file role="data" name="security.yml" />
					</dir>
					<dir name="templates">
						<file role="data" name="accessTokenSuccess.php" />
						<file role="data" name="authorizeFormSuccess.php" />
						<file role="data" name="authorizeInvalidSuccess.php" />
						<file role="data" name="authorizeRefusedSuccess.php" />
						<file role="data" name="authorizeSuccess.php" />
						<file role="data" name="requestTokenSuccess.php" />
					</dir>
				</dir>
			</dir>
    </dir>
  </contents>
 
  <dependencies>
    <required>
      <php>
        <min>5.3.0</min>
      </php>
      <pearinstaller>
        <min>1.4.1</min>
      </pearinstaller>
      <package>
        <name>symfony</name>
        <channel>pear.symfony-project.com</channel>
        <min>1.2.0</min>
        <max>1.3.0</max>
        <exclude>1.3.0</exclude>
      </package>
    </required>
  </dependencies>
 
  <phprelease>
  </phprelease>

  <changelog>
    <release>
      <version>
        <release>1.0.0</release>
        <api>1.0.0</api>
      </version>
      <stability>
        <release>beta</release>
        <api>beta</api>
      </stability>
      <license uri="http://www.symfony-project.com/license">MIT license</license>
      <date>2012-04-13</date>
      <license>MIT</license>
      <notes>
        * emerick42: initial release
      </notes>
    </release>
  </changelog>
</package>